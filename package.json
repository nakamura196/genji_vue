{
  "name": "genji_vue",
  "version": "1.0.2",
  "description": "Digital Tale of Genji",
  "author": "Satoru Nakamura",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "dev3": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "dev2": "nuxt-ts dev --port 8006",
    "build": "nuxt-ts build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt-ts generate",
    "build:gh-pages": "cross-env DEPLOY_ENV=GH_PAGES nuxt build",
    "generate:gh-pages": "cross-env DEPLOY_ENV=GH_PAGES nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "generate:staging": "NODE_ENV=\"staging\" nuxt generate",
    "generate:production": "NODE_ENV=\"production\" nuxt generate",
    "deploy:staging": "firebase deploy --only hosting",
    "release": "npm version patch && git push origin master && git push origin --tags"
  },
  "dependencies": {
    "@nuxt/content": "^1.15.0",
    "@nuxtjs/axios": "^5.13.1",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/google-analytics": "^2.4.0",
    "@nuxtjs/moment": "^1.6.1",
    "@nuxtjs/vuetify": "^1.11.3",
    "@types/chart.js": "^2.9.32",
    "chart.js": "^3.1.1",
    "firebase": "^8.4.1",
    "firebase-tools": "^9.10.0",
    "npm-check-updates": "^11.5.1",
    "nuxt": "^2.15.4",
    "nuxt-i18n": "^6.26.0",
    "push-dir": "^0.4.1",
    "vue-chartjs": "^3.5.1"
  },
  "devDependencies": {
    "@mdi/js": "^5.9.55",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxt/typescript-runtime": "^2.1.0",
    "@nuxtjs/eslint-config": "^6.0.0",
    "@nuxtjs/eslint-config-typescript": "^6.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/sitemap": "^2.4.0",
    "babel-eslint": "^10.1.0",
    "concurrent-transform": "^1.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-nuxt": ">=2.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.9.0",
    "gulp": "^4.0.2",
    "gulp-awspublish": "^4.1.2",
    "gulp-cloudfront-invalidate-aws-publish": "^1.0.0",
    "nodemon": "^2.0.7",
    "nuxt-leaflet": "^0.0.25",
    "nuxt-property-decorator": "^2.9.1",
    "nuxt-start": "^2.15.4",
    "prettier": "^2.2.1",
    "vue2-leaflet-markercluster": "^3.1.0",
    "vuex-module-decorators": "^1.0.1"
  }
}
